ifneq ($(CROSS-COMPILE),1)
OnboardATCSC.inl OnboardATCSC.h OnboardATCSS.h OnboardATCSS.cpp OnboardATCSC.cpp: $(subst $(SPACE),\$(SPACE),$(call ADD_ARCH,$(ACE_ROOT)/lib/$(LIB_PREFIX)TAO_IDL_BE.$(SOEXT))) $(subst $(SPACE),\$(SPACE),$(call ADD_ARCH,$(ACE_ROOT)/lib/$(LIB_PREFIX)TAO_IDL_FE.$(SOEXT)))
